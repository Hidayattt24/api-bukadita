# Environment variables for production deployment
# Copy this file to .env and fill in your actual values

# Server Configuration
PORT=4000

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Superadmin Configuration (for bootstrap)
SUPERADMIN_EMAIL=superadmin@bukadita.com
SUPERADMIN_PASSWORD=your_secure_password_here
SUPERADMIN_NAME=Super Admin
SUPERADMIN_PHONE=082283055874

# Optional: CORS Configuration (Frontend URL)
# FRONTEND_URL=https://your-frontend-domain.vercel.app

# Optional: JWT Configuration (if using Supabase Auth, this is not needed)
# JWT_SECRET=your_jwt_secret_here_min_32_chars_random_string

# Optional: Environment
# NODE_ENV=production
